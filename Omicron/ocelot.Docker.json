{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5200"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "otel-alpha",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/alpha/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete",
        "Options"
      ],
      "UpstreamHeaderTransform": {
        "X-Forwarded-PathBase": "/alpha"
      },
      "SwaggerKey": "alpha"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "otel-epsilon",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/epsilon/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete",
        "Options"
      ],
      "UpstreamHeaderTransform": {
        "X-Forwarded-PathBase": "/epsilon"
      },
      "SwaggerKey": "epsilon"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "otel-mu",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/mu/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete",
        "Options"
      ],
      "UpstreamHeaderTransform": {
        "X-Forwarded-PathBase": "/mu"
      },
      "SwaggerKey": "mu"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "otel-sigma",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/sigma/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete",
        "Options"
      ],
      "UpstreamHeaderTransform": {
        "X-Forwarded-PathBase": "/sigma"
      },
      "SwaggerKey": "sigma"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "alpha",
      "Config": [
        {
          "Name": "Alpha API",
          "Version": "v1",
          "Url": "http://otel-alpha/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "epsilon",
      "Config": [
        {
          "Name": "Epsilon API",
          "Version": "v1",
          "Url": "http://otel-epsilon/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "mu",
      "Config": [
        {
          "Name": "Mu API",
          "Version": "v1",
          "Url": "http://otel-mu/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "sigma",
      "Config": [
        {
          "Name": "Sigma API",
          "Version": "v1",
          "Url": "http://otel-sigma/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}